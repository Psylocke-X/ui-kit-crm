{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "jsx": "react-jsx",
    "declaration": true,
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "types": ["@storybook/react-webpack5", "node"],
    "moduleResolution": "node"
  },
  "include": ["packages/*"],
  "paths": {
    "packages/*": ["packages/*"],
    "*.module.scss": ["*"]
  }
}
